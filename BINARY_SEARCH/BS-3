
#include <stdio.h>
int binary_search(int a[],int len,int t)
{
    int ans=0;
  int low =0,high=len-1;
  
  while(low<=high)
  {
      int mid=(low+high)/2;
      if(a[mid]>t)
      {
          ans=mid;
          high=mid-1;
      }
      else
      {
          low=mid+1;
      }
  }
    
   
    return ans;
}

int main() {
    
    int a[]={1,2,3,4,5,6,7,8,9};
    int target=2;
    int len=sizeof(a)/sizeof(a[0]);
    
    int result=binary_search(a,len,target);
    
   
    printf("%d",result);
    

    return 0;
}
