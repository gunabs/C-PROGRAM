// Online C compiler to run C program online
#include <stdio.h>

int binary_search(int N,int target,int a[])
{
    int ans=0;
    int low=0;
    int high=N-1;
    
    while(low<=high)
    {
        int mid=(low+high)/2;
        
        if(a[mid]==target)
        {
            if(a[mid+1]!=target)
            {
                return mid;
            }
            else
            {
             low=mid+1;   
            }
        }
        else if(a[mid]>target)
        {
            high-=1;
        }
        else
        {
            low+=1;
        }
    }
    return -1;
}

int main() {
    
    int N = 7, target=28, array[] = {3,4,13,13,13,20,40};
    
    int result=binary_search(N,target,array);
    
    printf("%d ",result);

    return 0;
}
