
#include <stdio.h>

int binary_search(int a[],int len,int t)
{
    int left=0;
    int right=len-1;
    
    while(left<right)
    {
        int mid=(left+right)/2;
        
        if(t==a[mid])
        {
            return mid;
        }
        else if(t<a[mid])
        {
            right=mid-1;
        }
        else
        {
            left=mid+1;
        }
    }
    
}
int main() {
    
    int a[]={2,5,6,7,9,10,13,45};
    int target=45;
    int len=sizeof(a)/sizeof(a[0]);
    
    int result=binary_search(a,len,target);
    
    if(result==-1)
    {
        printf("target element not present in the array");
    }
    else
    {
         printf("target element present in the array ith %d ",result);
    }
    
    
    

    return 0;
}
