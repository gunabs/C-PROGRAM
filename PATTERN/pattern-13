
#include <stdio.h>
#define N 5
int main() {
    
   
      for(int i=0;i<N;i++){
        
          for(int j=0;j<=i;j++){
              
             if((i+j)%2==0)
             {
                 printf("1 ");
                 
             }
             else
             printf("0 ");
             
          }
          
          printf("\n");
          
      } 
    return 0;

}
