
#include <stdio.h>
#define N 5
int main() {
    
    
      for(int i=1;i<=2*N-1;i++){
        
          int stars = i;
         
          if(i>N) 
          stars = 2*N-i;
          
          for(int j=1;j<=stars;j++){
              printf("*");
          }
          printf("\n");
      } 
    return 0;

}
