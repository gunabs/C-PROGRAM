#include <stdio.h>


   



int main() {
    
  int a[]={3,1,2,4,2};
  
  int n=5;
  int k=6;
  int sum=0;
  int count=0;
  for(int i=0;i<n;i++)
  {
      if(sum<k)
      {
          sum=sum+a[i];
      }
      else
      {
          sum=sum-a[i];
          sum+=a[i];
      }
      if(sum==k)
      {
          count++;
      }
  }
    
    
printf("%d ",count);

    return 0;
}

