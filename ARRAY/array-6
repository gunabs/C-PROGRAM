
#include <stdio.h>

int main() {
    
     int a []= {1, 0, 2, 3, 2, 0, 0, 4, 5, 1};
        int n = 10;
        
       int j = -1;
       
        for (int i = 0; i < n; i++) 
        {
            if (a[i] == 0) 
            {
                j = i;
                break;
            }
        }

      
        if (j == -1) 
        {
            printf(" no zeros");        }

       
        for (int i = j + 1; i < n; i++)
        {
            if (a[i] != 0) 
            {
              
                int tmp = a[i];
                a[i] = a[j];
                a[j] = tmp;
                j++;
            }
        }
            
           
        
        
        
        for(int i=0;i<n;i++)
        {
            printf("%d ",a[i]);
        }

    return 0;
}
