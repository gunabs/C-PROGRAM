
#include <stdio.h>

int main() {
    
    
int prices[] = {1, 1, 0, 1, 1, 1};
int n=6;
int count=0;
int max=0;
for(int i=0;i<n;i++)
{
    
    if(prices[i]==1)
    {
        count++;
    }
    else
    {
        count=0;
    }
   if(count>max)
   {
       max=count;
   }
    
    
}
    
   printf("%d ",max); 
    
    
    


    return 0;
}
