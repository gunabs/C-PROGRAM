#include <stdio.h>

void setZeroes(int matrix[3][3], int m, int n) {
    
    int solution[m][n];
    
    int index=n-1;
    
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            solution[j][index]=matrix[i][j];
            
        }
        index--;
    }
    
    
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            matrix[i][j]=solution[i][j];
        }
    }
    
   

}

int main() {
    int matrix[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    int m = 3, n = 3;

    setZeroes(matrix, m, n);
    
    
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            printf("%d ",matrix[i][j]);
        }
        printf("\n");
    }
    
    
    


    return 0;
}

