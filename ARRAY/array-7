
#include <stdio.h>

int main() {
    
   int  n = 5,m = 5;
int arr1[] = {1,2,3,4,5}  ;
int arr2[] = {2,3,4,4,5};


int i=0,j=0;

while(i<n&&j<m)
{
    
    
    if(i>0&&arr1[i]==arr1[i-1])
    {
        i++;
        continue;
    }
     if(j>0&&arr2[j]==arr2[j-1])
    {
        j++;
        continue;
    }
    
    
    
    if(arr1[i]<arr2[j])
    {
        printf("%d ",arr1[i]);
        i++;
    }
    else if(arr2[j]>arr1[i])
    {
        printf("%d ",arr1[j]);
        j++;
    }
    else
    {
        printf("%d ",arr1[i]);
        i++;
        j++;
    }
    
    
    
}

while(i<n)
{
    if(i==0||arr1[i]!=arr1[i-1])
    {
        printf("%d ",arr1[i]);
    }
    i++;
}

while(j<n)
{
    if(j==0||arr2[j]!=arr2[j-1])
    {
        printf("%d ",arr2[j]);
    }
    j++;
}
      
    return 0;
}
