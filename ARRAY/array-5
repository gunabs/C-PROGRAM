
#include <stdio.h>
#include <limits.h>

int main() {
   
   int arr[]={5,2,7,4,9,1};
   int n=6;
  int  min=INT_MAX;
  int second_min=INT_MAX;
  
   for(int i=0;i<n;i++)
   {
       if(min>arr[i])
       {
           second_min=min;
           min=arr[i];
       }
       else if(min<arr[i]&&second_min>arr[i])
       {
           second_min=arr[i];
       }
       
   }
   
   
   printf("%d ",second_min);

    return 0;
}
