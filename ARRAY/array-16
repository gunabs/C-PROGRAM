
#include <stdio.h>
#include <limits.h>

int main() {
    
    int a[]={7,1,3,5,6,2};
    
    int min=INT_MAX;
    int max=INT_MIN;
    int profit=0;
    for(int i=0;i<6;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
            
        }
        profit=a[i]-min;
        
        if(profit>max)
        {
            max=profit;
        }
        
    }
    printf("%d ",max);

    return 0;
}
