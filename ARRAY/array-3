
#include <stdio.h>
#include <limits.h>

int main() {
   
   int arr[]={5,2,7,4,9,1};
   int n=6;
  int  max=INT_MIN;
  int second_max=INT_MIN;
  
   for(int i=0;i<n;i++)
   {
       if(max<arr[i])
       {
           second_max=max;
           max=arr[i];
       }
       else if(max>arr[i]&&second_max<arr[i])
       {
           second_max=arr[i];
       }
       
   }
   
   
   printf("%d ",second_max);

    return 0;
}
