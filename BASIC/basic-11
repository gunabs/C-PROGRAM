
#include <stdio.h>

int searcharray(int a[],int len,int element)
{
    for(int i=0;i<len;i++)
    {
        if(a[i]==element)
        {
            return i;
        }
    }
    return -1;
}


int main() {
  
   int a[]={12,45,2,65,34,23,9};
   int len=sizeof(a)/sizeof(a[0]);
   int element=65;
   
   int result=searcharray(a,len,element);
   
   if(result==-1)
   {
       printf("element not found");
   }
   else
   {
       printf("element found on %d th index",result);
   }
   
  
   

    return 0;
}
