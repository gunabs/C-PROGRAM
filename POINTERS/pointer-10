#include <stdio.h>
int main(){
    int arr[5]={5,1,4,2,3};
    int *p=arr,temp;
    for(int i=0;i<5;i++)
{
        for(int j=i+1;j<5;j++)
{
            if(*(p+i)>*(p+j)){
                temp=*(p+i);
                *(p+i)=*(p+j);
                *(p+j)=temp;
            }
}
}
    for(int i=0;i<5;i++) 
         printf("%d ",arr[i]);
    return 0;
}

