#include <stdio.h>

int main() {
    char str1[] = "hello";
    char str2[] = "hello";
    char *p1 = str1, *p2 = str2;
    int flag = 1;

    while (*p1 != '\0' && *p2 != '\0') {
        if (*p1 != *p2) {
            flag = 0;
            break;
        }
        p1++; p2++;
    }

    if (flag && *p1 == '\0' && *p2 == '\0')
        printf("Strings are equal\n");
    else
        printf("Strings are not equal\n");

    return 0;
}

