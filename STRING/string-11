#include <stdio.h>

int main() {
    char str[100];
    int seen[256] = {0};  
    int i, j = 0;

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    
    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] == '\n') {
            str[i] = '\0';
            break;
        }
    }

    
    for (i = 0; str[i] != '\0'; i++) {
        if (seen[(unsigned char)str[i]] == 0) {  
            seen[(unsigned char)str[i]] = 1;
            str[j++] = str[i];  
        }
        
    }

    str[j] = '\0';  

    printf("String after removing duplicates: %s\n", str);
    return 0;
}

