#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char s[1000];
    printf("Enter a num string: ");
    scanf("%s", s);

    int len = strlen(s);
    int index = -1;


    for (int i = len - 1; i >= 0; i--) {
        if ((s[i] - '0') % 2 == 1) {
            index = i;
            break;
        }
    }

    
    if (index == -1) {
        printf("No odd number.\n");
        return 0;
    }

    
    for (int i = 0; i <= index; i++) {
        printf("%c", s[i]);
    }
    printf("\n");

    return 0;
}

