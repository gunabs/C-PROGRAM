
#include <stdio.h>
#include<string.h>

int main() {
    char str[]="this is an amazing program";
    char *a[100];
    int i=0;
char *token=strtok(str," ");

while(token!=NULL)
{
    a[i++]=token;
   token= strtok(NULL," ");
}
   for(int j=i-1;j>=0;j--)
   {
       printf("%s ",a[j]);
   }
    
    return 0;
}
