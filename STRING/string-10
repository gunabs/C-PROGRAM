#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int freq[256] = {0};  
    int i, j;
    char tempChar;
    int tempFreq;

    
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

   
    str[strcspn(str, "\n")] = '\0';
    
    for (i = 0; str[i] != '\0'; i++) {
        freq[(unsigned char)str[i]]++;
    }

    
    char unique[256];
    int k = 0;
    for (i = 0; i < 256; i++) {
        if (freq[i] != 0) {
            unique[k++] = (char)i;
        }
    }


    for (i = 0; i < k - 1; i++) {
        for (j = i + 1; j < k; j++) {
            if (freq[(unsigned char)unique[i]] < freq[(unsigned char)unique[j]]) {
             
                tempFreq = freq[(unsigned char)unique[i]];
                freq[(unsigned char)unique[i]] = freq[(unsigned char)unique[j]];
                freq[(unsigned char)unique[j]] = tempFreq;

               
                tempChar = unique[i];
                unique[i] = unique[j];
                unique[j] = tempChar;
            }
        }
    }

   
    
    for (i = 0; i < k; i++) {
        printf("'%c' â†’ %d times\n", unique[i], freq[(unsigned char)unique[i]]);
    }

    return 0;
}

